@model IEnumerable<Handicraft_Website.Models.Address>
@{
    ViewData["Title"] = "Home Page";
    Layout = "_Customer";
}
<div class="page-wrapper">
    <section class="page-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="content">
                        <h1 class="page-name">Dashboard</h1>
                        <ol class="breadcrumb">
                            <li><a href="index.html">Home</a></li>
                            <li class="active">my account</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="user-dashboard page-wrapper">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <ul class="list-inline dashboard-menu text-center">
                        <li><a asp-controller="Customer" asp-action="ProfileOrders">Orders</a></li>
                        <li><a class="active" asp-controller="Customer" asp-action="ProfileAddress">Address</a></li>
                        <li><a asp-controller="Customer" asp-action="ProfileDetails">Profile Details</a></li>
                        <li><a asp-controller="Customer" asp-action="EditProfile">Edit Profile</a></li>
                        <li><a asp-controller="Customer" asp-action="EditPassword">Change Password</a></li>
                    </ul>
                    <div class="dashboard-wrapper user-dashboard">
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Address Line 1</th>
                                        <th>Address Line 2</th>
                                        <th>Country</th>
                                        <th>City</th>
                                        <th>Zip code</th>
                                        <th>Phone Number</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td></td>
                                            <td>@item.FirstName</td>
                                            <td>@item.LastName</td>
                                            <td>@item.AddressLine1</td>
                                            <td>@item.AddressLine2</td>
                                            <td>@item.Country</td>
                                            <td>@item.City</td>
                                            <td>@item.ZipCode</td>
                                            <td>@item.PhoneNumber</td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>